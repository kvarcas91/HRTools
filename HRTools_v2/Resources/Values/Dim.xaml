<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:core="clr-namespace:System;assembly=mscorlib">

    <!-- Font Sizes -->
    <core:Double x:Key="MainFontSize">14</core:Double>
    <core:Double x:Key="SearchPrimaryFontSize">12</core:Double>
    <core:Double x:Key="PrimaryDataFontSize">12</core:Double>
    <core:Double x:Key="SearchSecondaryFontSize">10</core:Double>
    <core:Double x:Key="SecondaryFontSize">10</core:Double>
    <core:Double x:Key="IntermediateFontSize">11</core:Double>
    <core:Double x:Key="DashboardValueHeader">17</core:Double>
    <core:Double x:Key="DashboardMainValue">22</core:Double>
    <core:Double x:Key="DashboardSecondaryValue">16</core:Double>

    <core:Double x:Key="PrimaryListFontSize">12</core:Double>
    <core:Double x:Key="SecondaryListFontSize">10</core:Double>

    <core:Double x:Key="NoDataFontSize">22</core:Double>

    <core:Double x:Key="TimelineDayFontSize">16</core:Double>
    <core:Double x:Key="TimelineDateFontSize">12</core:Double>
    <core:Double x:Key="TimelineMessageFontSize">12</core:Double>

    <core:Double x:Key="AwalInputFontSize">12</core:Double>

    <core:Double x:Key="SettingsHeaderFontSize">16</core:Double>

    <!-- Widths -->
    <core:Double x:Key="DashboardSeparatorWidth">100</core:Double>
    
</ResourceDictionary>